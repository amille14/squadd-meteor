@SquaddLogo = React.createClass
  mixins: [Destruct]

  getDefaultProps: ->
    color: false
    stroked: true
    fileType: "png"
    includeIcon: true
    includeText: false

  render: ->
    if @props.includeIcon
      src = "/images/squadd-logo"
      src += "-stroked" if @props.stroked
      src += "-bw" unless @props.color
      src += ".#{@props.fileType}"

    props = @destruct(@props, ["color", "stroked", "fileType", "includeIcon", "includeText"])

    return (
      <div ref="logo" className="squadd-logo" {...props._other}>
        {@_getLogoIcon(src) if @props.includeIcon}
        {@_getLogoText() if @props.includeText}
      </div>
    )

